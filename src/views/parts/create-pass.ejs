<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <!-- sr-only, ficará invisível para o usuário, porém visível para motores de busca, bom para acessibilidade -->
    <label class="sr-only" for="room-pass">Insira uma senha</label>
    <input
      type="password"
      id="room-pass"
      name="password"
      placeholder="Insira uma senha"
    />

    <button>
      <img src="/images/users 1.svg" alt="Criar sala" />
      Criar sala
    </button>
  </form>
</section>
